                        <th></th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr th:each="category : ${categoryService.getCategories()}">
                        <td>
                          <div class="d-flex px-2">
                            <div>
                              <img th:src="@{${category.getImage()}}" class="avatar avatar-sm rounded-circle me-2" th:alt="${category.getCategoryName()}">
                            </div>
                            <div class="my-auto">
                              <h6 class="mb-0 text-sm" th:text="${category.getCategoryName()}"></h6>
                            </div>
                          </div>
                        </td>
                        <td>
                          <p class="text-sm font-weight-bold mb-0" th:text="${category.getDescription()}"></p>
                        </td>
                        <td>
                          <span class="text-xs font-weight-bold" th:text="${productService.loadProductsByCategory(category).size()}"></span>
                        </td>
  <!--                      <td class="align-middle text-center">-->
  <!--                        <div class="d-flex align-items-center justify-content-center">-->
